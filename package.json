{
  "name": "zeed-dom",
  "version": "0.9.16",
  "description": "🌱 Lightweight offline DOM",
  "author": {
    "name": "Dirk Holtwick",
    "email": "dirk.holtwick@gmail.com",
    "url": "https://holtwick.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holtwick/zeed-dom.git"
  },
  "license": "MIT",
  "funding": {
    "type": "GitHub Sponsors ❤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "bugs": {
    "url": "https://github.com/holtwick/zeed-dom/issues"
  },
  "homepage": "https://github.com/holtwick/zeed-dom",
  "engines": {
    "node": ">=14.13.1"
  },
  "type": "module",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/index.js",
      "require": "./dist/index.cjs",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "sideEffects": false,
  "devDependencies": {
    "css-what": "^5.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "jest": "^27.3.1",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tsup": "^5.8.0",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run build:tsup -- --watch",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup src/index.ts --dts --sourcemap --format esm,cjs",
    "check": "tsc --noEmit -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm test && npm run build",
    "test": "jest src"
  },
  "files": [
    "dist"
  ]
}